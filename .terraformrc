provider_installation {
  network_mirror {
    url = "https://artifactory.nebius.dev/artifactory/api/terraform/terraform-provider/providers/"
    include = ["registry.terraform.io/internal/nebius"]
  }

  network_mirror {
    url = "https://artifactory.nebius.dev/artifactory/api/terraform/terraform-registry-provider-private/providers/"
    include = ["registry.terraform.io/nebius/ycp", "registry.terraform.io/nebius/ytr"]
  }
  network_mirror {
    url = "https://artifactory.nebius.dev/artifactory/api/terraform/terraform-registry-proxy/providers/"
    include = ["registry.terraform.io/*/*"]
    exclude = ["registry.terraform.io/nebius/*"]
  }
  direct {
    exclude = ["registry.terraform.io/*/*"]
  }
}

